<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<HBox fx:id="pantalla" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="servicios.ControladorPortada" stylesheets="@styles.css">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ImageView fitWidth="500" fitHeight="500" preserveRatio="true">
        <image>
            <Image url="@logo.png" />
        </image>
    </ImageView>

    <VBox alignment="CENTER">
        <Label fx:id="anho" text="1998"/>
        <Label fx:id="titulo" text="Taco Paco"/>
        <Button fx:id="btn_entrar" text="Entrar" prefWidth="300" maxWidth="300" textFill="white"
                onAction="#verMesas"/>
    </VBox>


</HBox>
